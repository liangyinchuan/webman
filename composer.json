  },
  "require": {
    "php": ">=7.2",
    "workerman/webman-framework": "^1.3.0",
    "monolog/monolog": "^2.0",
    "adhocore/jwt": "^1.1",
    "webman/action-hook": "^1.0",
    "tinywan/jwt": "^1.2",
    "twig/twig": "^3.3",
    "illuminate/redis": "^8.83",
    "symfony/cache": "^5.4",
    "illuminate/database": "^8.83",
    "workerman/crontab": "^1.0"
  },
  "suggest": {
    "ext-event": "For better performance. "
  },
  "autoload": {
    "psr-4": {
      "": "./",
      "App\\": "./app"
